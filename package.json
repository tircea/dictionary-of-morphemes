{
  "name": "morphology-analyzer",
  "version": "1.0.0",
  "description": "Ukrainian morphology analyzer with frontend and backend",
  "scripts": {
    "start": "node scripts/start.js",
    "start:backend": "cd backend && npm run dev",
    "start:frontend": "cd frontend && npm start",
    "install:all": "npm install && cd backend && npm install && cd ../frontend && npm install",
    "build:frontend": "cd frontend && npm run build",
    "start:backend:prod": "cd backend && npm run start:prod",
    "serve:frontend": "cd frontend && npx serve -s build",
    "start_prod": "node scripts/start-prod.js"
  },
  "devDependencies": {
    "serve": "^14.2.1"
  }
} 